{
  "name": "docosaurus-rex",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start-marp": "env PORT=5000 marp --html --allow-local-files --theme slides/theme.css --watch --server --allow-local-files slides",
    "build": "docusaurus build",
    "build-marp": "marp --html --allow-local-files --theme slides/theme.css -I slides --output build/slides",
    "copy-marp-images": "cp -r slides/images build/slides/images",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@agentofuser/remark-oembed": "^1.0.4",
    "@docusaurus/core": "^2.0.1",
    "@docusaurus/preset-classic": "^2.0.1",
    "@marp-team/marp-cli": "^2.1.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prettier": "^2.7.1",
    "prism-react-renderer": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lite-youtube-embed": "^2.3.1",
    "remark-html": "^15.0.1",
    "remark-oembed": "^1.2.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
